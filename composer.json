{
    "name": "ahmed-j-alsarem/saudi-riyal-filament-currency",
    "description": "A Laravel package for formatting and displaying Saudi Riyal currency in Filament applications with full Arabic support",
    "type": "library",
    "license": "MIT",
    "version": "1.1.5",
    "autoload": {
        "psr-4": {
            "AhmedJAlsarem\\SaudiRiyal\\FilamentCurrency\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ahmed J Alsarem",
            "email": "ahmed.j.alsarem@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^12.0",
        "filament/filament": "^3.0",
        "filament/support": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AhmedJAlsarem\\SaudiRiyal\\FilamentCurrency\\SaudiRiyalSymbolServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 